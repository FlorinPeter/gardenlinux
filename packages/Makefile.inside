all: apt cyrus-sasl2 dracut ignition iproute2 pam python3.9 linux-5.10 linux-5.10-signed
	@true

linux-5.10-signed: linux-5.10

%:
	if [ "$$ARCH" != "$$(dpkg --print-architecture)" ]; then \
		DEB_BUILD_OPTIONS="nocheck nodoc terse" DEB_BUILD_PROFILES="cross" manual/$@; \
	else \
		DEB_BUILD_OPTIONS="nodoc terse" manual/$@; \
	fi
